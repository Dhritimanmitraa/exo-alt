{
  "version": "1.0.0",
  "models": {
    "earthlike": "/models/earthlike_v1/model.json",
    "weirdness": "/models/weirdness_v1/model.json"
  },
  "features": [
    {
      "name": "pl_rade",
      "description": "Planet radius in Earth radii",
      "normalization": { "type": "minmax", "min": 0, "max": 10 }
    },
    {
      "name": "pl_bmasse",
      "description": "Planet mass in Earth masses (log scale)",
      "normalization": { "type": "log1p" }
    },
    {
      "name": "pl_insol",
      "description": "Stellar insolation (log scale)",
      "normalization": { "type": "log1p" }
    },
    {
      "name": "pl_eqt",
      "description": "Equilibrium temperature in Kelvin",
      "normalization": { "type": "minmax", "min": 0, "max": 3000 }
    },
    {
      "name": "sy_dist",
      "description": "System distance in parsecs (log scale)",
      "normalization": { "type": "log1p" }
    },
    {
      "name": "pl_orbper",
      "description": "Orbital period in days (log scale)",
      "normalization": { "type": "log1p" }
    }
  ],
  "metadata": {
    "trainedOn": "NASA Exoplanet Archive snapshot",
    "date": "2024-12-01",
    "earthlike": {
      "accuracy": 0.91,
      "auroc": 0.95,
      "notes": "Balanced dataset with class weighting to mitigate imbalance"
    },
    "weirdness": {
      "accuracy": 0.88,
      "auroc": 0.93,
      "notes": "Outlier-aware training using robust loss"
    }
  }
}


